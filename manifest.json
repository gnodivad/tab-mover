{
    "manifest_version": 2,
    "name": "Tab Mover",
    "version": "1.0",
    "description": "Move tabs between windows",
    "background": {
        "scripts": [
            "tab.js"
        ]
    },
    "commands": {
        "move-tabs": {
            "suggested_key": {
                "default": "Alt+Z"
            },
            "description": "Move"
        }
    }
}